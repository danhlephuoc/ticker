high at T :- value(V), alpha(V) at T [t 300 sec], leq(18,V).
mid at T :- value(V), alpha(V) at T [t 300 sec], leq(12,V), lt(V,18).
low at T :- value(V), alpha(V) at T [t 300 sec], lt(V,12).
lfu :- high always [t 300 sec].
lru :- mid always [t 300 sec].
fifo :- low always [t 300 sec], rtm50 [t 300 sec].
done :- lfu.
done :- lru.
done :- fifo.
random :- not done.
